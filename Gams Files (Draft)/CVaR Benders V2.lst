C o m p i l a t i o n


   1  sets
   2  crops /'wheat', 'corn', 'sugarbeets'/
   3  scenarios /lo,mid,hi/
   4  ;
   5   
   6  table yield(crops,scenarios) ’possible outcomes for demand’
   7              lo  mid hi
   8  wheat       2   2.5 3
   9  corn        2.4 3   3.6
  10  sugarbeets  16  20  24
  11  ;
  12   
  13  parameter alpha 'confidence level for CVaR';
  14  alpha = [1/3];
  15   
  16   
  17  parameter probability(scenarios) ’probability of each scenario materializing’
  18  /
  19  lo [1/3]
  20  mid [1/3]
  21  hi [1/3]
  22  /;
  23   
  24  parameter cost(crops) 'cost to plant per acre'
  25  /
  26  wheat 150,
  27  corn 230,
  28  sugarbeets 260
  29  /;
  30   
  31  *----------------------------------------------------------------------- * Form the Benders master problem *-----------------------------------------------------------------------
  32  ************************** Sets and Variables Needed to Perform Benders **************************
  33  sets iter ’max Benders iterations’ /iter1*iter2500/,
  34       dyniter(iter) ’dynamic subset’,
  35       dynscen(scenarios) 'dynamic subset of scenarios',
  36       curiter(iter) 'dynamic subset of iter',
  37       curscen(scenarios) 'scenario that the problem should look at'
  38  ;
  39   
  40  parameters oldgamma(iter),
  41            oldX(crops, iter),
  42            oldU(scenarios, iter),
  43            pi(scenarios, crops, iter),
  44            phi(scenarios, iter);
  45   
  46   
  47  ************************** Initial Master Problem **************************
  48  positive variables x(crops) '# of acres of land to allocate to wheat, corn, and sugarbeets';
  49   
  50  free variables zmaster ’objective variable of master problem’,
  51                 theta ’extra term in master obj’,
  52                 gamma 'VaR'
  53  ;
  54   
  55  equations masterobj ’master objective function’,
  56            plantamount 'constraint on first stage variables'
  57  ;
  58   
  59  masterobj..
  60      zmaster =e= sum(crops, cost(crops) * x(crops)) + gamma + theta;
  61   
  62  plantamount..
  63      sum(crops, x(crops)) =l= 500;
  64   
  65  gamma.lo = -5000000000;
  66  theta.lo = -5000000000;
  67   
  68  model initialmasterproblem /masterobj, plantamount/;
  69   
  70  ************************** Subproblem **************************
  71  positive variables p(crops, scenarios) '# of tons of each type of crops to sell in each scenario',
  72                     y(crops, scenarios) '# of tons of wheat/corn to buy in each scenario',
  73                     u(scenarios) 'auxiliary variables for reformulation of CVaR',
  74                     subX(crops)
  75  ;
  76   
  77  free variables zsub,
  78                 subgamma;
  79   
  80  equations subobj(scenarios),
  81            CVaRConstraint(scenarios),
  82            wheatfeeding(scenarios),
  83            cornfeeding(scenarios),
  84            sellingBeets(scenarios),
  85            setx(crops, iter),
  86            setgamma,
  87            CVaRConstraintDebug(scenarios)
  88            ;
  89   
  90  * Lots of equations here are indexed by scenarios, but there should only be one active scenario at a time.
  91  subobj(curscen)..
  92      zsub =e= [1/(1-alpha)] * (probability(curscen) * u(curscen));
  93   
  94  CVaRConstraint(curscen)..
  95      u(curscen) =g= (238 * y('wheat', curscen) + 210 * y('corn', curscen) - 170 * p('wheat', curscen) - 150 * p('corn', curscen) - 36 * p('sugarbeets', curscen)) - subgamma;
  96   
  97  CVaRConstraintDebug(curscen)..
  98      u(curscen) =g= (238 * y('wheat', curscen) + 210 * y('corn', curscen) - 170 * p('wheat', curscen) - 150 * p('corn', curscen) - 36 * p('sugarbeets', curscen));
  99   
 100  wheatfeeding(curscen)..
 101      yield('wheat', curscen) * subX('wheat') + y('wheat', curscen) - p('wheat', curscen) =g= 200;
 102   
 103  cornfeeding(curscen)..
 104      yield('corn', curscen) * subX('corn') + y('corn', curscen) - p('corn', curscen) =g= 240;
 105   
 106  sellingBeets(curscen)..
 107      p('sugarbeets', curscen) =l= yield('sugarbeets', curscen) * subX('sugarbeets');
 108   
 109  setx(crops, curiter)..
 110      subX(crops) =e= oldX(crops, curiter);
 111   
 112  setgamma(curiter)..
 113      subgamma =e= oldgamma(curiter);
 114   
 115  model subproblem /subobj, CVaRConstraint, wheatfeeding, cornfeeding, sellingBeets, setx, setgamma/;
 116   
 117  ************************** Master Problem **************************
 118  equation cutcon(iter);
 119   
 120  cutcon(dyniter)..
 121      theta =g= sum(scenarios, [1/(1-alpha)] * (probability(scenarios) * oldU(scenarios, dyniter))) +
 122                sum(crops, sum(scenarios, pi(scenarios, crops, dyniter) * (x(crops) - oldx(crops, dyniter)))) +
 123                sum(scenarios, phi(scenarios, dyniter) * (gamma - oldgamma(dyniter)));
 124   
 125   
 126  model masterproblem /masterobj, plantamount, cutcon/;
 127   
 128  ************************** Benders Algorithm **************************
 129  *
 130  * Step 1: Solve master problem without cuts
 131  *
 132  dyniter(iter) = NO;
 133  curiter(iter) = NO;
 134  curscen(scenarios) = NO;
 135  solve initialmasterproblem minimizing zmaster using lp;
 136  display zmaster.l;
 137   
 138  scalar lowerbound /-INF/;
 139  scalar upperbound /INF/;
 140  parameter objsub(scenarios);
 141  scalar objmaster;
 142  objmaster = zmaster.l - theta.l;
 143  display objmaster;
 144   
 145  * Save the x and gamma values
 146  oldX(crops, 'iter1') = x.l(crops);
 147  oldgamma('iter1') = gamma.l;
 148   
 149   
 150  oldU(scenarios, 'iter1') = 0;
 151  pi(scenarios, crops, 'iter1') = 0;
 152  phi(scenarios, 'iter1') = 0;
 153   
 154  option limrow = 0;
 155  option limcol = 0;
 156  subproblem.solprint = 0;
 157  masterproblem.solprint = 0;
 158   
 159  scalar counter /0/;
 160      loop(iter,
 161  *If not first iteration, run master problem
 162      if (ord(iter) <> 1,
 163  * Step 4: Solve masterproblem, and go back to step 2
 164   
 165          solve masterproblem minimizing zmaster using lp;
 166  * Save values from master problem
 167          oldX(crops, iter) = x.l(crops);
 168          oldgamma(iter) = gamma.l;
 169          lowerbound = zmaster.l;
 170          objmaster = zmaster.l-theta.l;
 171          display lowerbound, x.l;
 172  * Update curiter
 173          counter = counter + 1;
 174      );
 175   
 176  *
 177  * Step 2: Solve subproblems
 178  *
 179      dyniter(iter) = yes;
 180      curiter(iter) = yes;
 181      loop(scenarios,
 182          curscen(scenarios) = yes;
 183          solve subproblem minimizing zsub using lp;
 184          display x.l;
 185          display p.l, y.l;
 186  * Save values from this subproblem
 187          oldU(curscen, iter) = u.l(curscen);
 188          pi(scenarios, crops, iter) = setx.m(crops, iter);
 189          phi(scenarios, iter) = setgamma.m(iter);
 190          objsub(scenarios) = zsub.l;
 191          curscen(scenarios) = no;
 192      );
 193   
 194  upperbound = min(upperbound, objmaster + [1/(1-alpha)] * sum(scenarios, probability(scenarios) * oldU(scenarios, iter)));
 195   
 196  *
 197  * Step 3: Check for convergence
 198  *
 199      display lowerbound,upperbound;
 200      if (abs(upperbound - lowerbound) < 10,
 201      break;
 202      );
 203  curiter(iter) = no;
 204   
 205  );


